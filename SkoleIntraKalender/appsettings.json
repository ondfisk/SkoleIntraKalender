{
  "CalendarService": {
    "BaseAddress": "https://nfs.m.skoleintra.dk/",
    "UrlFormatString": "parent/[parentId:int]/[childName:string]/calendareventsource/LessonsEvents?className={0}&start={1}&end={2}&_={3}",
    "ClassName": "[className:string]",
    "CookieName": ".AspNet.ApplicationCookie",
    "CookieValue": "[Store in secrets.json for development]",
    "CookieDomain": "nfs.m.skoleintra.dk",
    "Password": "[Generate with Guid.NewGuid().ToString(\"n\"). Store in secrets.json for development]"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
